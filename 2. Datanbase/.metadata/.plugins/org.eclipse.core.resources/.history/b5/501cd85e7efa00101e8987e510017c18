
SELECT * FROM employees;
SELECT * FROM salaries;
SELECT * FROM dept_emp;
SELECT * FROM departments;

Q1~Q5 ###################################################

SELECT e.emp_no, s.salary FROM employees e, salaries s; 
SELECT e.emp_no, e.first_name, de.dept_no FROM employees e, dept_emp de; 

SELECT e.emp_no, e.first_name, de.to_date 
FROM employees e
JOIN dept_emp de
ON e.emp_no = de.emp_no 
JOIN departments dep
ON de.dept_no = dep.dept_no 
WHERE to_date = '9999-01-01'; 

SELECT first_name, salary
FROM employees e
JOIN salaries s
ON e.emp_no = s.emp_no
WHERE salary >= 80000; 

SELECT first_name, salary
FROM employees e
JOIN salaries s
ON e.emp_no = s.emp_no
ORDER BY salary DESC; 

SELECT * FROM employees;
SELECT * FROM salaries;
SELECT * FROM dept_emp;
SELECT * FROM departments;







Q6 ~ Q10 ######################################

SELECT * FROM departments d 

SELECT d. dept_name, count(e.emp_no) 
FROM departments d
JOIN dept_emp e ON d.dept_no = e.dept_no  
GROUP BY d.dept_name; 

SELECT d. dept_name, count(e.emp_no) 
FROM departments d
JOIN dept_emp e ON d.dept_no = e.dept_no  
WHERE emp_no >= 20000 
GROUP BY d.dept_name;

SELECT e.gender, e.first_name, de.dept_name
FROM employees e
JOIN dept_emp d
ON e.emp_no = d.emp_no 
JOIN departments de
ON d.dept_no = de.dept_no 
WHERE e.gender = 'F'; 

SELECT first_name, dept_name, to_date  
FROM employees e
JOIN dept_emp d
ON e.emp_no = d.emp_no  
JOIN departments de 
ON d.dept_no = de.dept_no  
WHERE to_date >= '1995-01-01'; 


SELECT * FROM employees;
SELECT * FROM salaries;
SELECT * FROM departments;

SELECT d. dept_name, avg(s.salary) AS avg_salary
FROM departments d
JOIN salaries s ON e.emp_no = s.emp_no   
GROUP BY d. dept_name;











