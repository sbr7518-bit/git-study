SELECT * FROM employees;

SELECT * FROM employees
WHERE gender = 'M';

SELECT * FROM employees
WHERE hire_date >= '1990-12-31';

SELECT * FROM employees
WHERE first_name = 'Georgi';

SELECT * FROM employees
WHERE first_name = 'Georgi';

SELECT * FROM employees
WHERE emp_no  = 10010;

SELECT dept_name FROM departments
WHERE dept_name = 'Tech Sales';





